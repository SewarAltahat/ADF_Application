    <SQLQuery><![CDATA[SELECT   DEPARTMENTS_PK , GET_DEPARTMENT_NAME(DEPARTMENTS_PK ,?)  as DISPLAY_DEPARTMENTS_PK                         
 from Hcm_departments                     
 where   dept_status_fl =  1      
 and    
 (            
  ? IS NOT NULL             
  OR             
  (             
  ? = ?                            
  or (? = ? and  DEPARTMENTS_PK = ?)                            
  or (? = ? and (DEPARTMENTS_PK = ? or DEPARTMENTS_PK IN (SELECT HIER.CHILD_DEPARTMENT FROM HCM_DEPARTMENTS_HIERARCHY HIER WHERE HIER.PARENT_DEPARTMENT = ?)))                                 
  )            
  )            
  AND            
    (            
    DEPT_SECURITY_FK IN             
   (SELECT SECURITY_LEVELS_FK FROM SSO_USER_DEPARTMENTS_SECURITY_LEVELS_VW VW            
    WHERE VW.USERS_PK = ? 
   )
 )            
 ORDER BY DEPARTMENTS_PK]]></SQLQuery>
