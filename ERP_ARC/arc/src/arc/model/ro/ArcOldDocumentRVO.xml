  <SQLQuery><![CDATA[select   OLD_DOCUMENT_PK   ,   
GET_DOCUMENT_SUBJECT(OLD_DOCUMENT_PK, ?) DISPLAY_OLD_DOCUMENT_PK,  
DOCUMENT_SECURITY_FK , 
FROM_DEPARTMENT_FK, 
TO_DEPARTMENT_FK 
from Arc_Old_Document where          
 (       
  ? IS NOT NULL        
  OR        
  (
  (        
  ? = ?                       
  or (? = ? and FROM_DEPARTMENT_FK= ?)                       
  or (? = ? and (FROM_DEPARTMENT_FK= ? or FROM_DEPARTMENT_FK
  IN (SELECT HIER.CHILD_DEPARTMENT FROM HCM_DEPARTMENTS_HIERARCHY HIER WHERE HIER.PARENT_DEPARTMENT = ?)))                            
  )   
  
  OR
    (        
  ?= ?                       
  or (? = ? and TO_DEPARTMENT_FK= ?)                       
  or (?= ? and (TO_DEPARTMENT_FK= ? or TO_DEPARTMENT_FK
  IN (SELECT HIER.CHILD_DEPARTMENT FROM HCM_DEPARTMENTS_HIERARCHY HIER WHERE HIER.PARENT_DEPARTMENT = ?)))                            
  ) 
  )
  )       
  AND       
    (       
    DOCUMENT_SECURITY_FK IS NULL OR       
  DOCUMENT_SECURITY_FK IN        
   (SELECT SECURITY_LEVELS_FK FROM SSO_USER_SECURITY_LEVELS_VW VW       
    WHERE VW.USERS_PK = ?      
    AND VW.SYSTEMS_FK = ?      
    AND VW.DEPARTMENTS_FK = ?       
  )         
  )       ]]></SQLQuery>
