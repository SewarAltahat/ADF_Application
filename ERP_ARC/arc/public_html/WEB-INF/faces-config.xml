<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee" version="2.1">
  <application>
    <default-render-kit-id>oracle.adf.rich</default-render-kit-id>
    <resource-bundle>
      <base-name>arc.view.scrlabels</base-name>
      <var>scrlabels</var>
    </resource-bundle>
    <!--<view-handler>gen.common.CustomViewHandler</view-handler>-->
    <locale-config>
      <default-locale>ar</default-locale>
    </locale-config>
  </application>
  <managed-bean>
    <managed-bean-name>menu</managed-bean-name>
    <managed-bean-class>arc.controller.bean.MenuBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>arcDocument</managed-bean-name>
    <managed-bean-class>arc.controller.bean.ArcDocumentBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>arcDocumentSearch</managed-bean-name>
    <managed-bean-class>arc.controller.bean.ArcDocumentSearchBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>scan</managed-bean-name>
    <managed-bean-class>arc.controller.bean.ScanBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>mainPage</managed-bean-name>
    <managed-bean-class>arc.controller.bean.MainPageBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>fileLocationsStandard</managed-bean-name>
    <managed-bean-class>arc.controller.bean.FileLocationsStandardBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>disposedDocuments</managed-bean-name>
    <managed-bean-class>arc.controller.bean.DisposedDocumentsBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>fileLocations</managed-bean-name>
    <managed-bean-class>arc.controller.bean.FileLocationsBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>borrowRequestsSearch</managed-bean-name>
    <managed-bean-class>arc.controller.bean.BorrowRequestsSearchBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>filesLocationTree</managed-bean-name>
    <managed-bean-class>arc.controller.bean.FilesLocationTreeBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
 
<managed-bean>
    <managed-bean-name>borrowRequests</managed-bean-name>
    <managed-bean-class>arc.controller.bean.BorrowRequestsBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>arcTaskList</managed-bean-name>
    <managed-bean-class>arc.controller.bean.ArcTaskListBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>arcDocumentView</managed-bean-name>
    <managed-bean-class>arc.controller.bean.ArcDocumentViewBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <navigation-rule>
    <navigation-case>
      <from-outcome>arcDocument</from-outcome>
      <to-view-id>/arcDocument.jspx</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>mainpage</from-outcome>
      <to-view-id>/mainpage.jspx</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>arcDocumentSearch</from-outcome>
      <to-view-id>/arcDocumentSearch.jspx</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>scan</from-outcome>
      <to-view-id>/scan.jspx</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>fileLocationsStandard</from-outcome>
      <to-view-id>/fileLocationsStandard.jspx</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>disposedDocuments</from-outcome>
      <to-view-id>/disposedDocuments.jspx</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>fileLocations</from-outcome>
      <to-view-id>/fileLocations.jspx</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>borrowRequestsSearch</from-outcome>
      <to-view-id>/borrowRequestsSearch.jspx</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>borrowRequests</from-outcome>
      <to-view-id>/borrowRequests.jspx</to-view-id>
    </navigation-case>
   <navigation-case>
      <from-outcome>arcTaskList</from-outcome>
      <to-view-id>/arcTaskList.jspx</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>arcDocumentView</from-outcome>
      <to-view-id>/arcDocumentView.jspx</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>arcDashboard</from-outcome>
      <to-view-id>/arcDashboard.jspx</to-view-id>
    </navigation-case>
  </navigation-rule>
  <managed-bean>
    <managed-bean-name>arcDashboard</managed-bean-name>
    <managed-bean-class>arc.controller.bean.ArcDashboardBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
</faces-config>