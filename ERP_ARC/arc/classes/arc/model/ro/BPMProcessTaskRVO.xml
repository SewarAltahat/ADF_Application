  <SQLQuery><![CDATA[SELECT TASK.Task_ID,                                
TASK.DEPLOYMENTID,                                
TASK.PROCESSID,                                
TASK.PROCESSINSTANCEID,                                
TASK.STATUS,                                
TASK.FORMNAME,                                
TASK.TASK_NAME,                                
TASK.ACTIVATIONTIME,                                
TASK.CREATEDON,                                          
TASK.ACTUALOWNER_ID,                               
GET_TASK_LATEST_TYPE(TASK.Task_ID) LAST_TYPE,                                
GET_TASK_LATEST_USER_ID(TASK.Task_ID) LAST_USERID,                                
GET_TASK_LATEST_OWNER_ID(TASK.Task_ID) LAST_OWNER,                              
TASK.ENTITY_ID,                              
GET_ACTOR_PASSWORD(TASK.ENTITY_ID) AS PASSWORD,                           
TASK.REQUEST_PK,                        
GET_ACTOR_DESC(TASK.ENTITY_ID, TASK.PROCESSID, ?) AS USER_DESC,           
GET_TASK_COMMENT(TASK.Task_ID) TASK_COMMENT,         
TASK.TASKFLOW_ID,      
REQUEST_DEPARTMENTS_FK,      
REQUEST_CATEGORIES_FK,     
REQUEST_SECURITY_FK     
FROM BPM_PROCESS_TASKS_VW TASK                              
where         
TASK.REQUEST_PK IS NOT NULL    
AND TASK.PROCESSID IN (SELECT PROCESSES_PK FROM BPM_PROCESSES WHERE SYSTEMS_FK = ? )  
ORDER BY TASK.Task_ID DESC]]></SQLQuery>
