    <SQLQuery><![CDATA[select FILES_LOCATION_PK,            
GET_FILES_LOCATION_FILE_LOCATI(FILES_LOCATION_PK, ?) DISPLAY_FILES_LOCATION_PK,        
DEPARTMENTS_FK,      
GET_DEPARTMENT_NAME(DEPARTMENTS_FK, ?) DISPLAY_DEPARTMENTS_FK      
from arc_files_location          
where files_location_pk  not in (select files_location_fk           
from arc_old_document where files_location_fk is not null           
AND (DOC_YEAR = ? OR ? IS NULL OR ?='')   
AND (USER_DEPARTMENT_FK = ? OR ? IS NULL  OR ? = '')  
AND (? IS NOT NULL                         
OR ( ? = ?                                      
OR (? = ? and  (USER_DEPARTMENT_FK = ? ) )            
OR (? = ? and (USER_DEPARTMENT_FK = ?             
OR USER_DEPARTMENT_FK IN (SELECT HIER.CHILD_DEPARTMENT FROM HCM_DEPARTMENTS_HIERARCHY HIER WHERE HIER.PARENT_DEPARTMENT = ?))) ))          
AND (EMPLOYEES_FK  = ? OR ? IS NULL  OR ? = '')          
AND (FILES_LOCATION_FK   = ? OR ? IS NULL  OR ? = '')          
AND (DOC_RECEIVE_DATE >= ? OR ? IS NULL)           
AND (DOC_RECEIVE_DATE <= ? OR ? IS NULL) )           
AND OPEN_FL  = 1 
AND (? IS NOT NULL                         
OR ( ? = ?                                      
OR (? = ? and  (DEPARTMENTS_FK = ? ) )            
OR (? = ? and (DEPARTMENTS_FK = ?             
OR DEPARTMENTS_FK IN (SELECT HIER.CHILD_DEPARTMENT FROM HCM_DEPARTMENTS_HIERARCHY HIER WHERE HIER.PARENT_DEPARTMENT = ?))) ))]]></SQLQuery>
