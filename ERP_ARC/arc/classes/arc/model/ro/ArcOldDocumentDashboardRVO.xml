  <SQLQuery><![CDATA[SELECT FILES_LOCATION_FK,   
COUNT(*) DOCUMENT_COUNT,   
GET_FILES_LOCATION_FILE_LOCATI(FILES_LOCATION_FK, ?) DISPLAY_FILES_LOCATION_PK      
FROM ARC_OLD_DOCUMENT     
WHERE FILES_LOCATION_FK IS NOT NULL    
AND (DOC_YEAR = ? OR ? IS NULL OR ? = ''  ) 
AND (USER_DEPARTMENT_FK = ? OR ? IS NULL  OR ? = '') 
AND (? IS NOT NULL                   
OR ( ? = ?                                
OR (? = ? and  (USER_DEPARTMENT_FK = ? ) )      
OR (? = ? and (USER_DEPARTMENT_FK = ?       
OR USER_DEPARTMENT_FK IN (SELECT HIER.CHILD_DEPARTMENT FROM HCM_DEPARTMENTS_HIERARCHY HIER WHERE HIER.PARENT_DEPARTMENT = ?))) ))        
AND (EMPLOYEES_FK  = ? OR ? IS NULL  OR ? = '')    
AND (FILES_LOCATION_FK   = ? OR ? IS NULL  OR ? = '')    
AND (DOC_RECEIVE_DATE >= ? OR ? IS NULL)     
AND (DOC_RECEIVE_DATE <= ? OR ? IS NULL)         
GROUP BY FILES_LOCATION_FK     
ORDER BY FILES_LOCATION_FK]]></SQLQuery>
